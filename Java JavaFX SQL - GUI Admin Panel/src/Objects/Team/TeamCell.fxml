<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" prefWidth="225.0" spacing="10.0" style="-fx-border-color: green; -fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Objects.Team.TeamCellController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <Text fx:id="klub" strokeType="OUTSIDE" strokeWidth="0.0" text="football club f.c.">
        <font>
            <Font name="Arial" size="12.0" />
        </font>
    </Text>
    <ImageView fx:id="flag" fitHeight="25.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
    <Text fx:id="miasto" strokeType="OUTSIDE" strokeWidth="0.0" text="c i t y">
        <font>
            <Font name="Arial" size="12.0" />
        </font>
    </Text>
</HBox>
